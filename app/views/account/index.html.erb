<h1>Listing Account</h1>


<table>
  <tr>
    <th>Type</th>
    <th>Balance</th>
  </tr>
 
  <% @account.each do |account| %>
    <tr>
      <td><%= account.type %></td>
      <td><%= account.balance %></td>
      <td><%= link_to 'Show', account_index_path(account) %></td>
    </tr>
  <% end %>
</table>